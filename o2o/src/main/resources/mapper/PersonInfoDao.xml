<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.pengan.dao.IPersonInfoDao">
    <select id="findPersonInfoById" parameterType="Long" resultType="PersonInfo">
        SELECT * FROM tb_person_info
        WHERE user_id = #{id};
    </select>

    <insert id="" keyColumn="local_auth_id" keyProperty="localAuthId" useGeneratedKeys="true" parameterType="PersonInfo">

    </insert>
</mapper>